    "@electron-forge/maker-zip": "^6.0.1",
    "@electron-forge/publisher-nucleus": "^6.0.3",
    "@vitejs/plugin-vue": "^3.1.0",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "electron": "^20.2.0",
    "less-loader": "7.x",
    "typescript": "^4.6.4",
